{
  "name": "@bookomol/workers",
  "version": "1.0.0",
  "description": "Background workers for Bookomol PDF processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:main": "node dist/main-worker.js",
    "start:chapter": "node dist/chapter-worker.js",
    "start:preview": "node dist/preview-worker.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.1.0",
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.2.1",
    "@prisma/client": "^5.7.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.0.379",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}